<entry name="Dropbox">
  <to>
    <member>{{ destination_zone }}</member>
  </to>
  <from>
    <member>{{ source_zone }}</member>
  </from>
  <source>
    <member>any</member>
  </source>
  <destination>
    <member>any</member>
  </destination>
  <source-user>
    <member>any</member>
  </source-user>
